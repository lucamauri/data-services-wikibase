{
  "api": {
    "sparql": {
      "uri": "/proxy/wdqs/bigdata/namespace/wdq/sparql"
    },
    "wikibase": {
      "uri": "$WIKIBASE_SCHEME://$WIKIBASE_HOST$WIKIBASE_API_PATH"
    },
    "examples": {
      "server": "$WIKIBASE_SCHEME://$WIKIBASE_HOST",
      "apiPath": "$WIKIBASE_API_PATH",
      "pageTitle": "$WIKIBASE_EXAMPLES_PAGE_TITLE",
      "pagePathElement": "/wiki/"
    },
    "urlShortener": "tinyurl",
    "query-builder": {
      "server": "https://gerrit.wikimedia.org/g/wikidata/query-builder"
    }
  },
  "brand": {
    "title": "$WIKIBASE_NAME Query Service",
    "logo": "$WIKIBASE_LOGO",
    "favicon": "$WIKIBASE_FAVICON",
    "copyrightUrl": "$WIKIBASE_COPYRIGHT",
    "index": "./index.html"
  },
  "prefixes": {
    "wd": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/entity/",
    "wdt": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/prop/direct/",
    "pq": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/prop/qualifier/"
  },
  "defaultPrefixes": {
    "wd": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/entity/",
    "wdt": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/prop/direct/",
    "pq": "$WIKIBASE_SCHEME://$WIKIBASE_HOST/prop/qualifier/"
  }
}
